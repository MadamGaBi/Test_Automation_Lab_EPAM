<?xml version="1.0"?>
<test name = "testMultPassLong">
    <data>
        <number1>3</number1>
        <number2>4</number2>
        <result>12</result>
    </data>
    <data>
        <number1>-2</number1>
        <number2>-7</number2>
        <result>14</result>
    </data>
    <data>
        <number1>0</number1>
        <number2>9</number2>
        <result>0</result>
    </data>
    <data>
        <number1>18</number1>
        <number2>0</number2>
        <result>0</result>
    </data>
    <data>
        <number1>4</number1>
        <number2>-3</number2>
        <result>-12</result>
    </data>
    <data>
        <number1>-5</number1>
        <number2>5</number2>
        <result>-25</result>
    </data>
    <!--The octal numeral system check-->
    <data>
        <number1>03</number1>
        <number2>04</number2>
        <result>12</result>
    </data>
    <data>
        <number1>-02</number1>
        <number2>-07</number2>
        <result>14</result>
    </data>
    <data>
        <number1>00</number1>
        <number2>06</number2>
        <result>0</result>
    </data>
    <data>
        <number1>014</number1>
        <number2>00</number2>
        <result>0</result>
    </data>
    <data>
        <number1>04</number1>
        <number2>-03</number2>
        <result>-12</result>
    </data>
    <data>
        <number1>-05</number1>
        <number2>05</number2>
        <result>-25</result>
    </data>
    <!--bounds check-->
    <!--Long.MIN_VALUE = -9223372036854775808, Long.MAX_VALUE = 9223372036854775807-->
    <data>
        <number1>2</number1>
        <number2>-4611686018427387904</number2>
        <result>-9223372036854775808</result>
    </data>
    <data>
        <number1>1</number1>
        <number2>9223372036854775807</number2>
        <result>9223372036854775807</result>
    </data>
</test>