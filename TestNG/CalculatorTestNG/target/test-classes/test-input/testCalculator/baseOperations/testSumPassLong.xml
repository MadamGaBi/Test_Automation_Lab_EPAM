<?xml version="1.0"?>
<test name = "testSumPassLong">
    <data>
        <number1>1</number1>
        <number2>1</number2>
        <result>2</result>
    </data>
    <data>
        <number1>-1</number1>
        <number2>-1</number2>
        <result>-2</result>
    </data>
    <data>
        <number1>0</number1>
        <number2>9</number2>
        <result>9</result>
    </data>
    <data>
        <number1>18</number1>
        <number2>0</number2>
        <result>18</result>
    </data>
    <data>
        <number1>1</number1>
        <number2>-1</number2>
        <result>0</result>
    </data>
    <data>
        <number1>-5</number1>
        <number2>5</number2>
        <result>0</result>
    </data>
    <data>
        <number1>1</number1>
        <number2>-17</number2>
        <result>-16</result>
    </data>
    <data>
        <number1>-43</number1>
        <number2>13</number2>
        <result>-30</result>
    </data>
    <!--The octal numeral system check-->
    <data>
        <number1>01</number1>
        <number2>01</number2>
        <result>2</result>
    </data>
    <data>
        <number1>-01</number1>
        <number2>-01</number2>
        <result>-2</result>
    </data>
    <data>
        <number1>00</number1>
        <number2>02</number2>
        <result>2</result>
    </data>
    <data>
        <number1>015</number1>
        <number2>00</number2>
        <result>15</result>
    </data>
    <data>
        <number1>01</number1>
        <number2>-01</number2>
        <result>0</result>
    </data>
    <data>
        <number1>-05</number1>
        <number2>05</number2>
        <result>0</result>
    </data>
    <data>
        <number1>01</number1>
        <number2>-017</number2>
        <result>-16</result>
    </data>
    <data>
        <number1>-043</number1>
        <number2>013</number2>
        <result>-30</result>
    </data>
    <!--bounds check-->
    <!--Long.MIN_VALUE = -9223372036854775808, Long.MAX_VALUE = 9223372036854775807-->
    <data>
        <number1>4611686018427387903</number1>
        <number2>4611686018427387904</number2>
        <result>9223372036854775807</result>
    </data>
    <data>
        <number1>-4611686018427387904</number1>
        <number2>-4611686018427387904</number2>
        <result>-9223372036854775808</result>
    </data>
    <data>
        <number1>9223372036854775807</number1>
        <number2>-9223372036854775808</number2>
        <result>-1</result>
    </data>
    <data>
        <number1>-9223372036854775808</number1>
        <number2>9223372036854775807</number2>
        <result>-1</result>
    </data>
</test>